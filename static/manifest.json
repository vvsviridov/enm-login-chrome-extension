{
  "manifest_version": 2,

  "name": "ENM Login Extension",
  "description": "This extension is for input login/password on ENM login page",
  "version": "0.0.1",

  "content_scripts": [
    {
      "matches": ["https://*/login/*"],
      "js": ["contentScript.js"]
    }
  ],

  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "content_security_policy": "script-src 'self' http://localhost:8080 'unsafe-eval'; object-src 'self'",
  "permissions": [
  ]
}
